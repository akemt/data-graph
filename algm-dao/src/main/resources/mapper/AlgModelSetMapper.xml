<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.beyond.algm.mapper.AlgModelSetMapper">
    <resultMap id="BaseResultMap" type="com.beyond.algm.model.AlgModelSet">
        <id column="model_set_sn" property="modelSetSn" jdbcType="VARCHAR"/>
        <result column="usr_sn" property="usrSn" jdbcType="VARCHAR"/>
        <result column="model_set_name" property="modelSetName" jdbcType="VARCHAR"/>
        <result column="model_orderby" property="modelOrderby" jdbcType="VARCHAR"/>
    </resultMap>
    <resultMap id="queryForListMap" type="com.beyond.algm.vo.AlgModelSetVo" extends="BaseResultMap">
    </resultMap>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
    delete from alg_model_set
    where model_set_sn = #{modelSetSn,jdbcType=VARCHAR}
  </delete>
    <insert id="insert" parameterType="com.beyond.algm.model.AlgModelSet">
    insert into alg_model_set (model_set_sn, usr_sn, model_set_name, model_orderby
      )
    values (#{modelSetSn,jdbcType=VARCHAR}, #{usrSn,jdbcType=VARCHAR}, #{modelSetName,jdbcType=VARCHAR}, #{modelOrderby,jdbcType=VARCHAR}
      )
  </insert>
    <update id="updateByPrimaryKey" parameterType="com.beyond.algm.model.AlgModelSet">
    update alg_model_set
    set usr_sn = #{usrSn,jdbcType=VARCHAR},
      model_set_name = #{modelSetName,jdbcType=VARCHAR},
      model_orderby = #{modelOrderby,jdbcType=VARCHAR}
    where model_set_sn = #{modelSetSn,jdbcType=VARCHAR}
  </update>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String">
    select model_set_sn, usr_sn, model_set_name, model_orderby
    from alg_model_set
    where model_set_sn = #{modelSetSn,jdbcType=VARCHAR}
  </select>
    <select id="selectAll" resultMap="BaseResultMap" parameterType="java.lang.String">
    select model_set_sn, usr_sn, model_set_name, model_orderby
    from alg_model_set
    where usr_sn = #{usrSn,jdbcType=VARCHAR}
  </select>

    <select id="queryModelSet" resultMap="queryForListMap" parameterType="java.lang.String">
        select model_set_sn, usr_sn, model_set_name, model_orderby
        from alg_model_set
        where usr_sn = #{usrSn,jdbcType=VARCHAR}
        order by model_orderby DESC
    </select>
    <sql id="sql_where">
        <where>
            <trim prefixOverrides="and">
            <if test="usrSn != null and usrSn!=''" >
                and  usr_sn = #{usrSn,jdbcType=VARCHAR}
            </if>
            <if test="modelSetSn != null and modelSetSn!=''" >
                and  model_set_sn = #{modelSetSn,jdbcType=VARCHAR}
            </if>
            <if test="modelSetName != null and modelSetName!=''" >
                and  model_set_name = #{modelSetName,jdbcType=VARCHAR}
            </if>
            </trim>
        </where>
    </sql>
    <select id="checkData" resultType="java.lang.Integer"  parameterType="com.beyond.algm.model.AlgModelSet"  >
        select COUNT(1)
        from alg_model_set
        <include refid="sql_where"/>
    </select>
    <select id="checkMaxOrderby" resultType="java.lang.String"  parameterType="java.lang.String"  >
        select max(model_orderby)
        from alg_model_set
        where usr_sn = #{usrSn,jdbcType=VARCHAR}
        group by usr_sn
    </select>

</mapper>