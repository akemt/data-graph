<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.beyond.algo.mapper.ArticleMapper" >
  <resultMap id="BaseResultMap" type="com.beyond.algo.model.Article" >
    <result column="AtlSn" property="atlsn" jdbcType="VARCHAR" />
    <result column="Title" property="title" jdbcType="VARCHAR" />
    <result column="Abstract" property="abstract" jdbcType="VARCHAR" />
    <result column="Keywords" property="keywords" jdbcType="VARCHAR" />
    <result column="DOI" property="doi" jdbcType="VARCHAR" />
    <result column="Author" property="author" jdbcType="VARCHAR" />
    <result column="CiteInfo" property="citeinfo" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.beyond.algo.model.Article" >
    insert into Article (AtlSn, Title, Abstract, 
      Keywords, DOI, Author, 
      CiteInfo)
    values (#{atlsn,jdbcType=VARCHAR}, #{title,jdbcType=VARCHAR}, #{abstract,jdbcType=VARCHAR}, 
      #{keywords,jdbcType=VARCHAR}, #{doi,jdbcType=VARCHAR}, #{author,jdbcType=VARCHAR}, 
      #{citeinfo,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap" >
    select AtlSn, Title, Abstract, Keywords, DOI, Author, CiteInfo
    from Article
  </select>
</mapper>